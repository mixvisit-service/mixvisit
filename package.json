{
  "private": true,
  "name": "mix-visit",
  "engines": {
    "node": ">=20.9.0"
  },
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "packages/*",
    "example"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "build:example": "yarn workspace @mix-visit/example run build",
    "build:web": "yarn workspace @mix-visit/web run build",
    "dev:example": "yarn workspace @mix-visit/example run dev",
    "dev:web": "yarn workspace @mix-visit/web run dev"
  },
  "devDependencies": {
    "xo": "^0.58.0"
  }
}
