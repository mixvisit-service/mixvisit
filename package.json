{
  "name": "mixvisit",
  "engines": {
    "node": ">=20.9.0"
  },
  "private": true,
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "packages/*",
    "example"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "build:example": "yarn workspace @mix-visit/example run build",
    "build:lite": "yarn workspace @mix-visit/lite run build",
    "dev:example": "yarn workspace @mix-visit/example run dev",
    "dev:lite": "yarn workspace @mix-visit/lite run dev"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.3"
  }
}
